                  <a class="dropdown-item" href="/manager/datlichsuccess">Đã cắt</a>
                  <a class="dropdown-item" href="/manager/datlichfail">Đã hủy</a>
                </div>
              </li>
              <li class="nav-item">
                <a class="nav-link btn-rotate" href="manager/logout">
                  <i class="fas fa-sign-out-alt"></i>
                  <p>
                    <span class="d-lg-none d-md-block">Account</span>
                  </p>
                </a>
              </li>
            </ul>
          </div>
        </div>
      </nav>
      <!-- End Navbar -->
      <!-- <div class="panel-header panel-header-sm">


</div> -->
      <div class="content">
        <div  class="row">
          <div class="col-md-12">
            <div style="width: 1250px" class="card">
              <div style="display: inline-block" class="card-header">
                <% if (success=='delete') { %>
                  <script>
                    swal({
                    title: "Delete success!",
                    icon: "success",
                    timer:1500,
                    button:false,
                      text:' '
                    }) 
                  </script>
                
                  <% } %>
                  <% if (success=='save') { %>
                    <script>
                      swal({
                      title: "Save success!",
                      icon: "success",
                      timer:1500,
                      button:false,
                      text:' '
                      }) 
                    </script>
                  
                    <% } %>
                    <% if (success=='edit') { %>
                      <script>
                        swal({
                        title: "edit success!",
                        icon: "success",
                        timer:1500,
                        button:false,
                      text:' '
                        }) 
                      </script>
                    
                      <% } %>


              </div>
              <div class="card-body">
                <table class="table text-center">
                  <thead class=" text-primary">
                    <th>
                      tên khánh hàng
                    </th>
                 
                    <th>
                     số điện thoại
                    </th>
                    <th class="text-right">
                      Thời gian cắt
                    </th>
                    <th >
                    Địa chỉ shop
                    </th>
                   
                      <th >
                       Trạng thái
                         </th>
                         <th >
                          Cập nhập trạng thái
                            </th>
                  </thead>
                  <tbody>
                      <% if (item) { %>
                        <% item.forEach(function(oders,index){ %>
                    <tr>
                      <td>
                        <%=oders.fullName%>
                       </td>
                       <td>
                         <%=oders.phoneNumber%>
                
                       </td>
                       <td>
                         <%=oders.time%> <%oders.date%>
                       </td>
                       <td>
                         <%=oders.place%>
                       </td>
                      
                       <td>
                        <% if (oders.__v==0) { %>
                            Chưa cắt
                          <% } %>
                          <% if (oders.__v==1) { %>
                           Đã cắt
                            <% } %>
                            <% if (oders.__v==2) { %>
                              Đã hủy
                               <% } %>
                       </td>
                      <td class="text-right">
                       
                          <form action="editbooklich?&id=<%=oders._id%>" id="mform" method="POST">
                          <table>
                            <tr>
                              <td>
                                <input style="width: 30px;" type="number" minlength="0" maxlength="2" value="<%=oders.__v%>" name="roleedit">
                              </td>
                              <td> <button type="submit"  class="btn btn-primary" >Cập nhập Trạng thái</button></td>
                            </tr>
                          </table>
                      
                          </div>
                          </form>
                          
                
                      </td>
                    
                    </tr>
                    <% }) %>
                    <% } %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
       
            </div>
          </div>
